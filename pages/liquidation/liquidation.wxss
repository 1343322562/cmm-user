.pageBox{padding-bottom: 110rpx;}
.confirm{background-color:#fff;width:90%;position:absolute;top:20%;left:5%;border-radius:15rpx;}
.confirm>.title{height:80rpx;line-height:80rpx;text-align:center;color:#999;border-bottom:1px solid #e8e8e8;font-size:26rpx;}
.confirm>.title.red{color: red;}
.confirm>.content{color:#999;width:90%;margin:0 auto;padding:20rpx 0;}
.confirm>.content .item{padding:5rpx 0;overflow:hidden;}
.confirm>.content .item>text{display:block;float:left;}
.confirm>.content .item>text:first-child{width:25%;text-align:right;}
.confirm>.content .item>text:last-child{width:75%;color:#FF0000}
.confirm>.btn{text-align:center;}
.confirm>.btn text{display:inline-block;border-radius:15rpx;margin:20rpx;width:40%;height:80rpx;line-height:80rpx;}
.confirm>.btn text:first-child{background-color:#f2f2f2;color:#999;}
.confirm>.btn text:last-child{background-color:#f49b1e;color:#fff;}
.downOrderBottom{background-color:#fff;position:fixed;left:0;bottom:0;width:100%;height:110rpx;border-top:1px solid #ebebeb;font-size:34rpx;}
.downOrderBottom cover-view{overflow:auto;}
.downOrderBottom .title{padding-left:20rpx;line-height:110rpx;height: 110rpx;}
.downOrderBottom .money{color:#FF0000;font-size:1.2em;position: absolute;top: 32rpx;left: 100rpx;width: 300rpx;}
.downOrderBottom .saveOrders{position:absolute;top:0;right:0;height:110rpx;background-color:#f49b1e;color:#fff;width:300rpx;text-align:center;line-height:110rpx;}
.downOrderBottom .saveOrders.act{background-color:  #FF0000!important;}
.remark{margin-top:20rpx;padding:0 20rpx 20rpx 20rpx;background-color:#fff;margin:20rpx 0; }
.remark>.contentTitle{height:100rpx;line-height:100rpx;position:relative}
.remark>.contentTitle:before{content:"";position:absolute;left:-20rpx;top:20%;width:3px;height:60%;background-color:#f49b1e}
.remark>.form{background-color:#f2f2f2;border-radius:10rpx;height:120rpx;padding:0 10rpx;width: 95%;}
.payWay{background-color:#fff;font-size:26rpx;position:relative;padding:30rpx 20rpx;margin-top: 20rpx;}
.payWay>.icon{width:45rpx;height:45rpx;vertical-align:middle;margin:0 10rpx;}
.payWay>.payWayTitle{color:#000;font-size:35rpx;}
.liquidation{background-color: #fff;padding-left: 20rpx;position: relative;height: 100rpx;}
.liquidation>.num{color: #999;font-size:26rpx;}
.liquidation>.num .text{margin-right: 20rpx;}
.liquidation>.total{padding-top: 12rpx;}
.liquidation>.total .money{color:red;font-size: 1.2em;}
.liquidation>.payMoeny{position: absolute;top: 0;right: 30rpx;line-height: 100rpx;}
.liquidation>.payMoeny .money{color: red;font-size: 1.3em;}
.li{position:relative;left:0;width:100%;overflow:hidden;border-bottom:1px solid #e8e8e8;}
.li>.goods{position:relative;left:0;background-color:#fff;padding: 15rpx 0;min-height: 150rpx;}
.li>.goods .img{width:150rpx;height:150rpx;position:absolute;overflow:hidden;left:20rpx;}
.li>.goods .img>.icon{width: 100%;height: 100%;}
.li>.goods .info{margin-left:190rpx;}
.li>.goods .info>.name{display:block;width:90%;overflow:hidden;display: -webkit-box; word-break: break-all; -webkit-box-orient: vertical; -webkit-line-clamp:2;text-overflow:ellipsis;}
.li>.goods .info>.name .gift{color: red;}
.li>.goods .info .now{font-size:35rpx;color:#FF0000;font-weight:bold}
.li>.goods .info  .before{color:#999;text-decoration:line-through;padding-left: 10rpx;}
.li>.goods .num{color: #999;font-size: 40rpx;position: absolute;top: 53%;right: 30rpx;font-weight: bold;}
.cartsType{line-height: 80rpx;color: #f49b1e;font-size: 36rpx;font-weight: bold;display: flex;justify-content: space-between;align-items: center;}
.cartsType.act{color: #FF0000!important}
.cartsType .icon{vertical-align: middle;width:50rpx;height: 50rpx;margin: 0 10rpx 0 10rpx;}
.cartsType .text{vertical-align: middle;}
.cartsType .cartsTypeC .self{color: rgb(73,99,252);} /* 自提点 */
.switchTransWayBtn {margin-right: 25rpx;font-size: 31rpx;border-radius: 30rpx;color: #f3f3f3;height: 60rpx;line-height: 60rpx;padding: 0 25rpx;}
.switchTransWayBtn.self{background: linear-gradient(130deg, rgb(136,115,250), rgb(73,99,252));} /* 自提点 */
.switchTransWayBtn.home{background: linear-gradient(130deg, rgb(255,161,105), rgb(254,99,76)); } /* 送到家 */
.orders{border-bottom: 1px solid #f2f2f2;background-color: #fff;position: relative;}
.orders>.user{padding: 20rpx 20rpx 15rpx 20rpx;clear: both;}
.orders>.user .text{margin-right: 15rpx;font-size: 30rpx;font-weight: bold}
.orders>.user .type{color: #f49b1e;display:inline-block;border: 1px solid #f49b1e;border-radius: 10rpx;padding: 5rpx 10rpx;margin-right: 20rpx;}
.orders>.user .type.act{color: #FF0000!important;border-color: #FF0000!important;}
.orders>.icon{width: 100%;height: 10rpx;margin-bottom: 5rpx;float: left;}
.orders>.goRight{position: absolute;top: 50%;right: 30rpx;width: 40rpx;height: 40rpx;margin-top: -20rpx;}
.express{padding: 0 20rpx; padding-right:60rpx;}
.express>.addr{font-size: 30rpx;padding: 10rpx 0 20rpx 0;font-weight: bold;}
.takeTheir>.shop{position: relative;padding-top: 10rpx!important; width: 85%;padding:0 20rpx ;}
.takeTheir>.shop .img{width: 100rpx;height: 100rpx;overflow: hidden;border-radius: 20rpx;position: absolute;top: 15rpx;left: 20rpx;}
.takeTheir>.shop .info{margin-left: 120rpx;padding-bottom: 10rpx;min-height: 105rpx;}
.takeTheir>.shop .info>.name{color: #666;font-size: 28rpx;}
.takeTheir>.shop .info>.addr{font-size: 30rpx;font-weight: bold}
.useCoupons{background-color: #fff;padding: 0 20rpx;line-height: 100rpx;border-top: 1px solid #e4e4e4;}
.useCoupons>.icon{width: 40rpx;height: 40rpx;vertical-align: middle;margin-right: 10rpx;}
.useCoupons>.right{float: right}
.useCoupons>.right .text{color: red;}
.orderType{display: flex;background-color: #fff;justify-content: center;}
.orderType>.item{width: 240rpx;text-align: center;padding: 37rpx 0;position: relative}
.orderType>.item.act{color: #f49b1e}
.orderType>.item .shippingFee{color: red;font-size: 22rpx;position: absolute;bottom: 10rpx;left: 124rpx;text-align: left;width: 200rpx}

.couponsSelect{background-color: #fff;overflow: hidden;padding: 0 20rpx;border-bottom: 1px solid #e8e8e8;line-height: 80rpx;}
.couponsSelect .text{vertical-align: middle;}
.couponsSelect>.type{float: left}
.couponsSelect>.type .icon{vertical-align: middle;width:40rpx;height: 40rpx;margin-right: 10rpx;}
.couponsSelect>.money{float: right;color: #666;font-size: 24rpx;padding-left: 20rpx;}
.couponsSelect>.money .icon{width: 45rpx;height: 45rpx;vertical-align: middle;}
.couponsSelect>.money .act{color: red}

.location_null{line-height: 80rpx;}
.location_null .icon2{vertical-align: middle;width: 35rpx;height: 35rpx; margin: 0 10rpx 10rpx 20rpx;}
.location_null .text{font-size: 35rpx;color: #f49b1e; }
.itemSize{color:#999}