.address_null{position: absolute;top: 0;left: 0;bottom: 0;width: 100%;height: 100%;background-color: #fff;z-index: 10;text-align: center;color: #999;}
.address_null>.icon{width: 160rpx;margin: 220rpx  0 30rpx 0 ;height: 208rpx;}
.address_null>.text{display: block;}
.addAddress{background-color: #FF5800;color: #fff;text-align: center;width: 80%;line-height: 80rpx;border-radius: 40rpx;position: fixed;left: 10%;bottom: 30rpx;font-size: 32rpx;z-index:50;}
.self{background: linear-gradient(130deg, rgb(136,115,250), rgb(73,99,252));}
.list{padding-bottom: 120rpx;}
.list>.li{background-color: #fff;padding-top: 10rpx;margin-bottom: 20rpx;position: relative;}
.list>.li.null::before{content:"不在配送范围内";color: red;position: absolute;left: 20rpx;bottom: 20rpx;}
.list>.li .user{overflow: hidden;color: #000;font-size: 32rpx;line-height: 65rpx;}
.list>.li .user>.phone{float: right;}
.list>.li .city{font-size: 26rpx;}
.list>.li .city>.text{margin-right: 30rpx;}
.list>.li .addr{font-size: 26rpx;}
.list>.li .block{padding: 0 20rpx;}
.list>.li .settings{border-top: 1px solid #e8e8e8;margin-top: 20rpx;overflow: hidden;line-height: 80rpx;color: #999;}
.list>.li .settings>.default{float: left;}
.list>.li .settings>.default .checkbox{width: 40rpx;height: 40rpx;overflow: hidden;border-radius: 100%;border:1px solid #e8e8e8;float: left;margin: 20rpx 20rpx 0 20rpx;}
.list>.li .settings>.default .checkbox.act{background-color: #FF5800;border-color: #FF5800;}
.list>.li .settings>.default .checkbox>.icon{width: 100%;height: 100%;float: left;}
.list>.li .settings>.change{float: right;}
.list>.li .settings>.change .btn{display: inline-block;padding: 0 20rpx;}
.list>.li .settings>.change .icon{width: 40rpx;height: 40rpx;vertical-align: middle;margin-right: 10rpx;}


