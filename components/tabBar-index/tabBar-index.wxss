/* 限时秒杀 */
.seckill{background-color: #fff;margin: 20rpx 0;position: relative;}
.seckill>.title{font-size:32rpx;font-weight:600;color:#000;line-height:80rpx;padding-left: 30rpx;}
.seckill>.countDown{position: absolute;top: 20rpx;right: 20rpx;font-size: 26rpx;}
.seckill>.countDown .date{text-align: center;display: inline-block;color: #ca2126;vertical-align: middle;}
.seckill>.countDown .date>.t{margin: 0 10rpx;display: inline-block;min-width: 45rpx;height: 40rpx;color: #fff;background-color: #ca2126;border-radius: 8rpx;line-height: 40rpx;}
.seckill .list{display: flex;padding-bottom: 20rpx;vertical-align: middle;}
.seckill .li{margin: 15rpx 0 0 15rpx;padding:15rpx;border-radius: 15rpx;flex-grow:0;position: relative;height: 360rpx;}
.seckill .li>.img{width:180rpx;height: 180rpx;margin: 0 auto;background-color: #f2f2f2;border-radius: 15rpx;overflow: hidden;display: block;position: relative}
.nullStock{position: absolute;width: 100rpx;line-height: 100rpx;border-radius: 100%;background-color: rgba(0, 0, 0,.4);top: 55rpx;left: 50%;color: #fff;text-align: center;font-size: 22rpx;margin-left: -50rpx;transform: rotate(-40deg)}
.nullStock::after,.nullStock::before{content: "";position:absolute;top: 50%;height: 1px;background-color: #999;width: 30rpx;}
.nullStock::after{left: -35rpx;}
.nullStock::before{right: -35rpx;}
.seckill .li .name{display: -webkit-box; word-break: break-all; -webkit-box-orient: vertical; -webkit-line-clamp:2; overflow: hidden; text-overflow:ellipsis;font-size: 25rpx;margin-top: 15rpx;width: 180rpx;line-height: 30rpx;}
.seckill .li>.info{position:absolute;left: 15rpx;bottom: 20rpx;}
.seckill .li .price{padding-top: 20rpx;}
.seckill .li .now{font-weight: bold;font-size: 32rpx;color: red;padding-right: 10rpx;}
.seckill .li .befored{color: #999;font-size: 24rpx;text-decoration:line-through;}
.seckill .li .btn{background: linear-gradient(90deg,#3baa5a, 50%, #57dc69);border-radius: 30rpx;color: #fff;text-align: center;width: 180rpx;line-height: 40rpx;font-size: 25rpx;}
.seckill .li .btn.act{background: linear-gradient(90deg,#ea1543, 50%, #fa6e4f);}
.seckill .li .btn.null{background: #909090;}
